<h1>All My Dishes</h1>

<table id="meals">
<tr>
<th>Dish</th>
<th>Day</th>
<th>Meal</th>

</tr>
<% @dishes.each do |dish| %>
    <%# <% binding.pry %>
    <% if current_user.owns_dish?(dish) %>

<tr>

<td><%= link_to dish.dish_name, dish_path(dish) %></td>
    <% dish.meals.each do |meal| %>
        <td><%= meal.day_name %></td>
        <td><%= meal.meal_time %></td>
        <% end %>
    <% end %>
<% end %>

</table>
